generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Athlete {
  id      String  @id
  athleteId  String  @unique
  gender  String  
  sport   String
  competition Competition[]
}

model Competition {
  id        String  @id
  athleteId  String
  athlete   Athlete @relation(fields: [athleteId], references: [id], onDelete: Cascade)
}

model Meta {
  id          Int     @id @default(autoincrement())
  userId      String  
  heartrate   Int

}
